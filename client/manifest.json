{
	"manifest_version": 2,
	"name": "My Cool Extension",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["./node_modules/jquery/dist/jquery.min.js", "content.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": ["background.js"]
	}
}
